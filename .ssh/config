Host *
    ControlMaster auto
    ControlPath ~/.ssh/session-%r@%h:%p
    ControlPersist 600
    ServerAliveInterval 60
    ServerAliveCountMax 3

Host github
    HostName github.com
    User git
    IdentityFile ~/.ssh/github
    #required to prevent ssh default behavior of sending id_rsa first
    IdentitiesOnly yes

Host garlic
    HostName garlic.acm.umn.edu
    IdentityFile ~/.ssh/acm
    IdentitiesOnly yes

Host argo
    HostName argo.acm.umn.edu
    IdentityFile ~/.ssh/acm
    IdentitiesOnly yes
